  "tokenColors": [

  // ===== 1Ô∏è‚É£ COMMENTS =====
  { "scope": ["comment"], "settings": { "foreground": "#008000", "fontStyle": "italic" } },
  { "scope": ["comment.line", "comment.block"], "settings": { "foreground": "#008000", "fontStyle": "italic" } },
  { "scope": ["comment.line.number-sign.shell"], "settings": { "foreground": "#008000", "fontStyle": "italic" } },
  { "scope": ["punctuation.definition.comment"], "settings": { "foreground": "#008000", "fontStyle": "" } },
  { "scope": ["comment.todo"], "settings": { "foreground": "#008000", "fontStyle": "bold" } },

  // ===== 2Ô∏è‚É£ STRINGS =====
  { "scope": ["string"], "settings": { "foreground": "#808080", "fontStyle": "" } },
  { "scope": ["string.quoted.single", "string.quoted.double"], "settings": { "foreground": "#808080", "fontStyle": "" } },
  { "scope": ["string.interpolated"], "settings": { "foreground": "#808080", "fontStyle": "" } },
  { "scope": ["string.unquoted"], "settings": { "foreground": "#808080", "fontStyle": "" } },
  { "scope": ["punctuation.definition.string.begin", "punctuation.definition.string.end"], "settings": { "foreground": "#808080", "fontStyle": "" } },

  // ===== 3Ô∏è‚É£ CONSTANTS =====
  { "scope": ["constant"], "settings": { "foreground": "#FF0000", "fontStyle": "" } },
  { "scope": ["constant.numeric"], "settings": { "foreground": "#FF0000", "fontStyle": "" } },
  { "scope": ["constant.language"], "settings": { "foreground": "#FF0000", "fontStyle": "" } },
  { "scope": ["constant.character"], "settings": { "foreground": "#FF0000", "fontStyle": "" } },
  { "scope": ["constant.other"], "settings": { "foreground": "#808080", "fontStyle": "" } },
  { "scope": ["constant.other.option.shell"], "settings": { "foreground": "#808080", "fontStyle": "" } },

  // ===== 4Ô∏è‚É£ KEYWORDS =====
  { "scope": ["keyword"], "settings": { "foreground": "#0000FF", "fontStyle": "bold" } },
  { "scope": ["keyword.control"], "settings": { "foreground": "#0000FF", "fontStyle": "bold" } },
  { "scope": ["keyword.operator"], "settings": { "foreground": "#0000FF", "fontStyle": "" } },
  { "scope": ["keyword.operator.assignment", "keyword.operator.arithmetic"], "settings": { "foreground": "#795E26", "fontStyle": "" } },
  { "scope": ["storage"], "settings": { "foreground": "#0000FF", "fontStyle": "bold" } },
  { "scope": ["storage.type"], "settings": { "foreground": "#0000FF", "fontStyle": "bold" } },
  { "scope": ["storage.modifier"], "settings": { "foreground": "#0000FF", "fontStyle": "bold" } },
  { "scope": ["storage.modifier.readonly.shell", "storage.modifier.declare.shell"], "settings": { "foreground": "#0000FF", "fontStyle": "bold" } },

  // ===== 5Ô∏è‚É£ VARIABLES =====
  { "scope": ["variable"], "settings": { "foreground": "#800080", "fontStyle": "" } },
  { "scope": ["variable.other"], "settings": { "foreground": "#800080", "fontStyle": "" } },
  { "scope": ["variable.other.assignment.shell"], "settings": { "foreground": "#000000", "fontStyle": "" } },
  { "scope": ["variable.parameter"], "settings": { "foreground": "#800080", "fontStyle": "italic" } },
  { "scope": ["variable.language"], "settings": { "foreground": "#800080", "fontStyle": "" } },
  { "scope": ["variable.other.readwrite"], "settings": { "foreground": "#800080", "fontStyle": "" } },
  { "scope": ["variable.other.positional.shell"], "settings": { "foreground": "#c42cc4", "fontStyle": "bold" } },
  { "scope": ["support.variable"], "settings": { "foreground": "#800080", "fontStyle": "" } },
  { "scope": ["meta.parameter.shell"], "settings": { "foreground": "#800080", "fontStyle": "italic" } },

  // ===== 6Ô∏è‚É£ ENTITIES =====
  { "scope": ["entity.name"], "settings": { "foreground": "#795E26", "fontStyle": "" } },
  { "scope": ["entity.name.function"], "settings": { "foreground": "#795E26", "fontStyle": "" } },
  { "scope": ["entity.name.type"], "settings": { "foreground": "#795E26", "fontStyle": "" } },
  { "scope": ["entity.name.command.shell"], "settings": { "foreground": "#0000FF", "fontStyle": "bold" } },
  { "scope": ["entity.other.attribute-name"], "settings": { "foreground": "#795E26", "fontStyle": "" } },
  { "scope": ["entity.other.attribute-name.bracket.shell"], "settings": { "foreground": "#795E26", "fontStyle": "" } },

  // ===== 7Ô∏è‚É£ SUPPORT (BUILT-INS) =====
  { "scope": ["support.function"], "settings": { "foreground": "#795E26", "fontStyle": "" } },
  { "scope": ["support.function.builtin.shell"], "settings": { "foreground": "#0000FF", "fontStyle": "bold" } }, // ===== echo, exit etc.
  { "scope": ["support.function.shell"], "settings": { "foreground": "#795E26", "fontStyle": "" } },
  { "scope": ["support.function.other.shell"], "settings": { "foreground": "#795E26", "fontStyle": "" } },
  { "scope": ["support.constant"], "settings": { "foreground": "#FF0000", "fontStyle": "" } },
  { "scope": ["support.type"], "settings": { "foreground": "#0000FF", "fontStyle": "" } },
  { "scope": ["support.class"], "settings": { "foreground": "#0000FF", "fontStyle": "" } },
  { "scope": ["support.other"], "settings": { "foreground": "#0000FF", "fontStyle": "" } },

  // ===== 8Ô∏è‚É£ PUNCTUATION / OPERATORS =====
  { "scope": ["punctuation"], "settings": { "foreground": "#202124", "fontStyle": "" } },
  { "scope": ["punctuation.definition"], "settings": { "foreground": "#FF0000", "fontStyle": "" } },
  { "scope": ["punctuation.definition.bracket.named-array.shell"], "settings": { "foreground": "#202124", "fontStyle": "" } },
  { "scope": ["punctuation.section"], "settings": { "foreground": "#202124", "fontStyle": "" } },
  { "scope": ["punctuation.separator"], "settings": { "foreground": "#202124", "fontStyle": "" } },
  { "scope": ["keyword.operator.assignment.shell"], "settings": { "foreground": "#795E26", "fontStyle": "bold" } },
  { "scope": ["keyword.operator.logical"], "settings": { "foreground": "#795E26", "fontStyle": "" } },
  { "scope": ["keyword.operator.arithmetic"], "settings": { "foreground": "#795E26", "fontStyle": "" } },

  // ===== 9Ô∏è‚É£ META =====
  { "scope": ["meta.function"], "settings": { "foreground": "#202124", "fontStyle": "" } },
  { "scope": ["meta.function-call"], "settings": { "foreground": "#202124", "fontStyle": "" } },
  { "scope": ["meta.function-call.shell"], "settings": { "foreground": "#795E26", "fontStyle": "" } },
  { "scope": ["meta.statement"], "settings": { "foreground": "#202124", "fontStyle": "" } },
  { "scope": ["meta.statement.command.name.shell"], "settings": { "foreground": "#0000FF", "fontStyle": "bold" } },
  { "scope": ["meta.brace"], "settings": { "foreground": "#202124", "fontStyle": "" } },
  { "scope": ["meta.block"], "settings": { "foreground": "#202124", "fontStyle": "" } },
  { "scope": ["source.shell"], "settings": { "foreground": "#202124", "fontStyle": "" } },

  // ===== ‚ö†Ô∏è INVALID / SPECIAL =====
  { "scope": ["invalid.illegal"], "settings": { "foreground": "#FFFFFF", "fontStyle": "bold" } },
  { "scope": ["invalid.deprecated"], "settings": { "foreground": "#FFFFFF", "fontStyle": "italic" } },

  // ===== üîü HEREDOC =====
  //{ "scope": ["keyword.operator.heredoc.shell"], "settings": { "foreground": "#ff000086", "fontStyle": "bold" } },
  { "scope": ["punctuation.definition.string.heredoc.delimiter.shell"], "settings": { "foreground": "#FF0000", "fontStyle": "bold" } },
  { "scope": ["string.quoted.heredoc", "string.unquoted.heredoc"], "settings": { "foreground": "#A0522D", "fontStyle": "" } },
 
  
  // =====  11 TODO =====
  { "scope": ["keyword.other.documentation.todo.comment.shell"], "settings": { "foreground": "#008000", "fontStyle": "bold" } },
  { "scope": ["keyword.other.documentation.todo"], "settings": { "foreground": "#008000", "fontStyle": "bold" } },
  { "scope": ["comment.block"], "settings": { "foreground": "#008000", "fontStyle": "bold" } },
  { "scope": ["comment.todo"], "settings": { "foreground": "#008000", "fontStyle": "" } },

  // =====  12 SHEBANG =====
  { "scope": ["meta.shebang.shell comment.line.number-sign.shell"], "settings": { "foreground": "#008000", "fontStyle": "bold" } },
  { "scope": ["meta.shebang.shell"], "settings": { "foreground": "#008000", "fontStyle": "bold" } },
  { "scope": ["punctuation.definition.comment.shebang.shell"], "settings": { "foreground": "#008000", "fontStyle": "bold" } },

  // =====  FORCE COMMENT OVERRIDES  =====
  {"scope": ["comment.line.number-sign.shell meta.shebang.shell", "comment.line.number-sign.shell", "comment.line.shell", "comment.shell", "comment" ],
    "settings": { "foreground": "#008000", "fontStyle": "bold italic" } }
  ]
}